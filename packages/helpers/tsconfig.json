{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": false,
		"moduleResolution": "node",
		"preserveWatchOutput": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"noEmit": true,
		"strict": true
	},
	"exclude": ["node_modules"]
}
